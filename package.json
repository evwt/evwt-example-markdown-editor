{
  "name": "evwt-example-markdown-editor",
  "version": "0.0.1",
  "author": "John Susek <john@johnsolo.net>",
  "description": "Example project for EVWT",
  "license": "Apache-2.0",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "xvfb-maybe mocha",
    "test:watch": "nodemon --exec mocha",
    "electron:watch": "nodemon --watch src/background --watch $EVWT_PATH/background --exec yarn electron:serve",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve --inspect=9229",
    "electron:inspect": "vue-cli-service electron:serve --inspect-brk=9229",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "dependencies": {
    "core-js": "^3.6.5",
    "evwt": "^0.0.18",
    "lodash": "^4.17.19",
    "markdown-it": "^11.0.0",
    "uuid": "^8.3.0",
    "vue": "^2.6.11",
    "vue-monaco": "https://github.com/johnsusek/vue-monaco"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "babel-eslint": "^10.1.0",
    "electron": "^9.2.0",
    "electron-devtools-installer": "^3.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "mocha": "^8.1.1",
    "monaco-editor-webpack-plugin": "^1.9.0",
    "robotjs": "^0.6.0",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "sendkeys-js": "https://github.com/johnsusek/node-sendkeys-js",
    "spectron": "^11.1.0",
    "vue-cli-plugin-electron-builder": "^2.0.0-rc.4",
    "vue-svg-loader": "^0.16.0",
    "vue-template-compiler": "^2.6.11",
    "xvfb-maybe": "^0.2.1"
  },
  "babel": {
    "presets": [
      "@vue/cli-plugin-babel/preset"
    ]
  }
}
